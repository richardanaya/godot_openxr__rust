config_version=5

[application]

config/name="Test OpenXR"
run/main_scene="res://main_scene.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[rendering]

renderer/rendering_method.mobile="forward_plus"
renderer/rendering_method.web="forward_plus"

[xr]

openxr/enabled=true
openxr/extensions/hand_tracking=false
openxr/extensions/eye_gaze_interaction=true
shaders/enabled=true
